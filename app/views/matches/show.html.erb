<% @match.teams.each do |team| %>
  <%= team.name -%></br/>
<% end %>
<% @match.games.each_with_index do |game, index| %>
  Game <%= index + 1 -%>
  <br/>
  Score <%= game.score %>
  <br/>
  Winning team: <%= game.winning_team.name %>
  <br/>
  <%= link_to "Edit", edit_match_game_path(@match, game) %>
<% end %>
<br/>
<%= link_to "New Game", new_match_game_path(@match) %>
